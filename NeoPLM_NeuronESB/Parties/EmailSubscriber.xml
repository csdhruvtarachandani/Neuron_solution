<?xml version="1.0"?>
<ESBSubscriber xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Id>3ea97fd2-5ceb-4fc6-a38a-b92977a133a2</Id>
  <Version>2481b6da-b506-4fce-adc7-64f56a65b640</Version>
  <BusinessFlows />
  <Description>A subscriber to receive the error email messages from business processes and pass it to the Adapter Endpoint to send emails.</Description>
  <Name>EmailSubscriber</Name>
  <Group>General</Group>
  <IsSystemObject>false</IsSystemObject>
  <IsPrivate>false</IsPrivate>
  <Bindings />
  <SubscriberId>EmailSubscriber</SubscriberId>
  <ClientGroup>General</ClientGroup>
  <Zone>Enterprise</Zone>
  <Enabled>true</Enabled>
  <Subscriptions>
    <Subscription>
      <TopicName>EmailNotification</TopicName>
      <CanRead>true</CanRead>
      <CanWrite>false</CanWrite>
      <SubscriptionType>Topic</SubscriptionType>
    </Subscription>
  </Subscriptions>
  <ACL />
  <Anonymous>true</Anonymous>
  <AllowMultipleInstances>true</AllowMultipleInstances>
  <LogActivity>false</LogActivity>
  <UseUniqueWmiCounterName>false</UseUniqueWmiCounterName>
</ESBSubscriber>